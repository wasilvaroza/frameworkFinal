<!DOCTYPE html>
<html>
<head>
  <title>Controle de Eventos</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	
	<%= render "layouts/navbar" %>
	
	<div class="container">
		<div class="row">
			<div class="col-md-3">
				<%= render "layouts/latbar" %>
			</div>
			<div class="col-md-9">
				<div class="panel-body">
					<%= yield %>
					<p class="notice"><%= notice %></p>
					<p class="alert"><%= alert %></p>
				</div>
			</div>
		</div> <!-- row -->
	</div> <!-- container -->
	

	<%= link_to 'Eventos' , eventos_path %> 
	<%= link_to 'Atividades' , atividades_path %> 
	<%= link_to 'Participantes' , participantes_path %> 

	
<body>
</html>
